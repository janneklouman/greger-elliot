{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.5.0",
		"silverstripe/cms": "3.5.x-dev",
		"silverstripe/framework": "3.5.x-dev",
		"silverstripe/reports": "3.5.x-dev",
		"silverstripe/siteconfig": "3.5.x-dev"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~4.8"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
